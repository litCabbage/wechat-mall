.container {
  background: #f2f2f2;
  padding-bottom: 100rpx;
}

form {
  width: 100%;
}

.address-box {
  width: 100%;
  background: url(data:image/png;base64,) no-repeat 705rpx center,
    url(data:image/png;base64,) no-repeat center bottom;
  background-color: #fff;
  margin: 20rpx 0;
}

.add-address {
  width: 100%;
}

.add-address .title {
  font-size: 28rpx;
  color: #000;
  margin-left: 30rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 60rpx auto;
  padding: 40rpx 0 40rpx 90rpx;
}

.show-address {
  width: 664rpx;
  box-sizing: border-box;
  padding-left: 100rpx;
  background: url(data:image/png;base64,) no-repeat 30rpx 35rpx;
  background-size: 30rpx auto;
}

.show-address .name-tel {
  font-size: 28rpx;
  color: #000;
  padding: 30rpx 0rpx 20rpx 0rpx;
}

.show-address .addr-text {
  font-size: 24rpx;
  color: #888;
  padding-bottom: 34rpx;
  line-height: 36rpx;
}

.goods-list {
  width: 100%;
  background: #fff;
  margin-bottom: 20rpx;
}

.goods-list .list-title {
  font-size: 28rpx;
  color: #000;
  padding: 30rpx 0rpx 25rpx 30rpx;
}

.goods-list .a-goods {
  width: 720rpx;
  margin-left: 30rpx;
  display: flex;
  border-top: 1px solid #eee;
  padding: 30rpx 30rpx 30rpx 0;
}

.goods-list .a-goods .img-box {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  margin-right: 20rpx;
  background: #d8d8d8;
}

.goods-list .img-box .img {
  width: 160rpx;
  height: 160rpx;
}

.goods-list .a-goods  .text-box {
  width: 510rpx;
  box-sizing: border-box;
  padding-top: 10rpx;
}

.a-goods .text-box .arow {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.a-goods .text-box .arow .goods-name {
  width: 360rpx;
  font-size: 26rpx;
  height: 74rpx;
  color: #000;
  line-height: 1.6;
  overflow: hidden;
}

.a-goods .text-box .arow01 {
  margin-bottom: 30rpx;
}

.arow .goods-price {
  font-size: 26rpx;
  color: #000;
  align-self: flex-start;
}

.arow .goods-label {
  font-size: 26rpx;
  color: #999;
}

.arow .goods-num {
  font-size: 26rpx;
  color: #999;
}